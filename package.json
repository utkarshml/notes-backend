{
  "name": "backend",
  "version": "1.0.0",
  "description": "TypeScript Backend with Authentication and Notes Management",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp package*.json dist/",
    "lint": "tsc --noEmit",
    "watch": "tsc --watch"
  },
  "keywords": [
